<GMap
  @mapId="<YOUR MAPID"
  @lat="51.507568"
  @lng="-0.127762"
  @zoom={{9}} as |map|>

  {{#each this.londonLocations as |location|}}
    <map.advancedMarker
      @lat={{location.lat}}
      @lng={{location.lng}}
      @gmpDraggable={{false}}
      @onClick={{this.onClick}} />
  {{/each}}

</GMap>